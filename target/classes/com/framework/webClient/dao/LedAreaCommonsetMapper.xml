<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedAreaCommonsetDao" >	<delete id="deleteLedAreaCommonsetById"  parameterType="map" >		DELETE FROM LED_AREA_COMMONSET WHERE FQID IN ( SELECT FQID FROM	LED_TEMPLATE_AREA WHERE MBID = #{mbid} )	</delete>	<insert id="insertLedAreaCommonset" parameterType="list" >		<foreach collection="list" item="map" index="index" open="begin" close=";end;" separator=";">			INSERT INTO LED_AREA_COMMONSET (				PZID,				FQID				<if test="map.pzz != null and map.pzz != ''">					,PZMC					,PZZ				</if>				<if test="map.wjid != null and map.wjid != ''">					,WJID				</if>				)  				VALUES 				(					SYS_GUID (),					#{map.fqid}					<if test="map.pzz != null and map.pzz != ''">						,#{map.pzmc}						,#{map.pzz}					</if>					<if test="map.wjid != null and map.wjid != ''">						,#{map.wjid}					</if>				) 		</foreach>	</insert></mapper>
<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedAreaFileDao" >	<resultMap id="BaseResultMap" type="com.framework.webClient.entity.LedAreaFile" >		<id column="WJID" property="wjid" />		<result column="FQID" property="fqid" />		<result column="XH" property="xh" />		<result column="BZ" property="bz" />	</resultMap>	<sql id="sql_select">		<![CDATA[			SELECT 				T.WJID,				T.FQID,				T.XH,				T.BZ			FROM LED_AREA_FILE  T 		]]>	</sql>	<select id="selectLedAreaFileById"  resultMap="BaseResultMap" parameterType="java.lang.Long" >		<include refid="sql_select"></include>		<where>			<if test=" id != null and  id != '' ">				AND T.WJID = #{id}			</if></where>	</select>	<delete id="deleteLedAreaFileById"  parameterType="map" >		DELETE FROM  LED_AREA_FILE WHERE FQID IN (SELECT FQID FROM LED_TEMPLATE_AREA WHERE MBID = #{mbid})	</delete>	<update id="updateLedAreaFileById"  parameterType="com.framework.webClient.entity.LedAreaFile" >		UPDATE  LED_AREA_FILE SET 			BZ = '${bz}'		WHERE WJID = #{wjid} AND 	</update>	<insert id="insertLedAreaFile" parameterType="list" >		INSERT INTO LED_AREA_FILE (			WJID,			FQID,			XH			) 			<foreach collection = "list" item = "map" index = "index" separator = "UNION ALL"> 				(					SELECT						#{map.wjid},						#{map.fqid},						#{map.xh}					FROM						DUAL				) 		</foreach>	</insert></mapper>
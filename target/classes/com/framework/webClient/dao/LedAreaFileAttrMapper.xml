<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedAreaFileAttrDao" >	<resultMap id="BaseResultMap" type="com.framework.webClient.entity.LedAreaFileAttr" >		<id column="SXID" property="sxid" />		<result column="FQID" property="fqid" />		<result column="WJID" property="wjid" />		<result column="SXMC" property="sxmc" />		<result column="SXZ" property="sxz" />		<result column="WJIDZ" property="wjidz" />		<result column="BZ" property="bz" />	</resultMap>	<sql id="sql_select">		<![CDATA[			SELECT 				T.SXID,				T.FQID,				T.WJID,				T.SXMC,				T.SXZ,				T.WJIDZ,				T.BZ			FROM LED_AREA_FILE_ATTR  T 		]]>	</sql>	<delete id="deleteLedAreaFileAttrById"  parameterType="map" >		DELETE FROM  LED_AREA_FILE_ATTR WHERE  FQID IN (SELECT FQID FROM LED_TEMPLATE_AREA WHERE MBID = #{mbid})	</delete>	<insert id="insertLedAreaFileAttr" parameterType="list" >		<foreach collection="list" item="map" index="index" open="begin" close=";end;" separator=";">			INSERT INTO LED_AREA_FILE_ATTR (				SXID,				FQID,				WJID,				SXMC,				SXZ				<if test="map.wjidz != null and map.wjidz != ''">					,WJIDZ				</if>									)  				VALUES 				(					SYS_GUID (),					#{map.fqid},					#{map.wjid},					#{map.sxmc},					#{map.sxz}					<if test="map.wjidz != null and map.wjidz != ''">						,#{map.wjidz}					</if>				)		</foreach> 	</insert>	</mapper>
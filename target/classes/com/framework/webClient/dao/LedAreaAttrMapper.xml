<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedAreaAttrDao" >	<delete id="deleteLedAreaAttrById"  parameterType="map" >		DELETE FROM	LED_AREA_ATTR WHERE	FQID IN ( SELECT FQID FROM LED_TEMPLATE_AREA WHERE MBID = #{mbid})	</delete>	<insert id="insertLedAreaAttr" parameterType="list" >		<foreach collection="list" item="map" index="index" open="begin" close=";end;" separator=";">			INSERT INTO LED_AREA_ATTR (				SXID,				FQID,				SXMC				<if test="map.sxz != null and map.sxz != ''">					,SXZ				</if>				<if test="map.wjid != null and map.wjid != ''">					,WJID				</if>				)  				VALUES				(				SYS_GUID (),				#{map.fqid},				#{map.sxmc}				<if test="map.sxz != null and map.sxz != ''">					,#{map.sxz}				</if>				<if test="map.wjid != null and map.wjid != ''">					,#{map.wjid}				</if>				)		</foreach>																																		</insert></mapper>
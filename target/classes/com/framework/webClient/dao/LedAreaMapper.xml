<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedAreaDao" >	<resultMap id="BaseResultMap" type="com.framework.webClient.entity.LedArea" >		<id column="FQID" property="fqid" />		<result column="FQLX" property="fqlx" />		<result column="BZ" property="bz" />	</resultMap>	<sql id="sql_select">		<![CDATA[			SELECT 				T.FQID,				T.FQLX,				T.BZ,				T.WJID			FROM LED_AREA  T 		]]>	</sql>	<select id="selectLedAreaById"  resultMap="BaseResultMap" parameterType="java.lang.Long" >		<include refid="sql_select"></include>		<where>			<if test=" id != null and  id != '' ">				AND T.FQID = #{id}			</if></where>	</select>	<delete id="deleteLedAreaById"  parameterType="map" >		DELETE FROM LED_AREA WHERE	FQID IN (SELECT FQID FROM	LED_TEMPLATE_AREA	WHERE	MBID = #{mbid}) 	</delete>	<update id="updateLedAreaById"  parameterType="com.framework.webClient.entity.LedArea" >		UPDATE  LED_AREA SET 			BZ = '${bz}'		WHERE FQID = #{fqid} AND 	</update>	<insert id="insertLedArea" parameterType="list" >		INSERT INTO LED_AREA (			FQID,			FQLX,			WJID			)  			<foreach collection = "list" item = "map" index = "index" separator = "UNION ALL"> 				(					SELECT						#{map.fqid},						#{map.fqlx},						#{map.wjid}					FROM						DUAL				) 		</foreach>	</insert></mapper>
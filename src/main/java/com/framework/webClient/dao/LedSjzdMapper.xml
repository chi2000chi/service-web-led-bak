<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedSjzdDao" >	<sql id="sql_select">		<![CDATA[			SELECT 				T.ID AS "id",				T.ZDBM AS "zdbm",				T.ZDMC AS "zdmc",				T.PZDBM AS "pzdbm"			FROM LED_SJZD  T 		]]>	</sql>	<select id="selectLedSjzdByPzdbm"  resultType="map" parameterType="String" >		<include refid="sql_select"></include>		<where>			<if test=" pzdbm != null and  pzdbm != '' ">				AND T.PZDBM = #{pzdbm}			</if></where>	</select>		<select id="selectLedSjzdByPzdbmList"  resultType="map" parameterType="map" >		<include refid="sql_select"></include>		<where>			<if test=" pzdbmList != null and  pzdbmList.size() > 0 ">				AND T.PZDBM in 				<foreach collection="pzdbmList" index="index" item="item" open="(" separator="," close=")">  			        '${item}'			    </foreach> 			</if>		</where>	</select></mapper>
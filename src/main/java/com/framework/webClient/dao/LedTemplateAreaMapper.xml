<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.framework.webClient.dao.LedTemplateAreaDao" >	<resultMap id="BaseResultMap" type="com.framework.webClient.entity.LedTemplateArea" >		<id column="GLID" property="glid" />		<result column="MBID" property="mbid" />		<result column="FQID" property="fqid" />		<result column="BZ" property="bz" />	</resultMap>	<sql id="sql_select">		<![CDATA[			SELECT 				T.GLID,				T.MBID,				T.FQID,				T.BZ			FROM LED_TEMPLATE_AREA  T 		]]>	</sql>	<select id="selectLedTemplateAreaById"  resultMap="BaseResultMap" parameterType="java.lang.Long" >		<include refid="sql_select"></include>		<where>			<if test=" id != null and  id != '' ">				AND T.GLID = #{id}			</if></where>	</select>	<delete id="deleteLedTemplateAreaById"  parameterType="map" >		DELETE FROM	LED_TEMPLATE_AREA WHERE	MBID = #{mbid}	</delete>	<update id="updateLedTemplateAreaById"  parameterType="com.framework.webClient.entity.LedTemplateArea" >		UPDATE  LED_TEMPLATE_AREA SET 			BZ = '${bz}'		WHERE GLID = #{glid} 	</update>	<insert id="insertLedTemplateArea" parameterType="list" >				INSERT INTO LED_TEMPLATE_AREA (			GLID,			MBID,			FQID			)  			<foreach collection = "list" item = "map" index = "index" separator = "UNION ALL"> 				(					SELECT						SYS_GUID (),						#{map.mbid},						#{map.fqid}					FROM						DUAL				) 		</foreach>										</insert></mapper>